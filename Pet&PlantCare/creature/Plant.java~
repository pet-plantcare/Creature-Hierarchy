public abstract class Plant extends Creature {
   
   //properties
   private int height;
   private String species;
   
   public Plant(
   //methods
   public int getHeight() {
      return height;
   }
   
   public void setHeight( int height)
   {
      this.height = height;
   }
   
   public String getSpecies ()
   {
      return species;
   }
   
   public void setSpecies(String species)
   {
      this.species = species;
   }   
}
